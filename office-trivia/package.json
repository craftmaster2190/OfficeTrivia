{
  "name": "office-trivia",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "spellcheck": "cspell \"src/**/*.{ts,js,html,scss}\" || exit 0",
    "format": "prettier --write \"src/**/*.{ts,js,json,html,css,scss}\" || exit 0",
    "deploy": "ng build --prod",
    "postdeploy": "aws s3 sync ./dist/office-trivia/ s3://office-trivia/  --profile office-trivia --delete"
  },
  "husky": {
    "hooks": {
      "commit-msg": "cspell $HUSKY_GIT_PARAMS || exit 0",
      "pre-commit": "yarn format && yarn spellcheck"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.0",
    "@angular/cdk": "^9.2.0",
    "@angular/common": "~9.0.0",
    "@angular/compiler": "~9.0.0",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~9.0.0",
    "@angular/material": "^9.2.0",
    "@angular/platform-browser": "~9.0.0",
    "@angular/platform-browser-dynamic": "~9.0.0",
    "@angular/router": "~9.0.0",
    "canvas-confetti": "^1.2.0",
    "materialize-css": "^1.0.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.1",
    "@angular/cli": "~9.0.1",
    "@angular/compiler-cli": "~9.0.0",
    "@angular/language-service": "~9.0.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "cspell": "^4.0.55",
    "husky": "^4.2.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "prettier": "^2.0.4",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5"
  }
}
